<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class catalog="db_rest_fel" name="cz.cvut.fel.restauracefel.hibernate.MenuItemType" table="menuitemtype">
    <id name="menuItemTypeId" type="java.lang.Integer">
      <column name="menuItemTypeID"/>
      <generator class="native"/>
    </id>
    <property name="name" type="string">
      <column name="name" not-null="true"/>
    </property>
    <property name="itemCount" type="int">
      <column name="itemCount" not-null="true"/>
    </property>
    <property name="isDeleted" type="int">
      <column name="isDeleted" not-null="true"/>
    </property>
  </class>
</hibernate-mapping>
